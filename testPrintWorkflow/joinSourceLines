import WDL
w = WDL.load('/home/ubuntu/repos/starfusion/starFusion.wdl')
# find all params that need to be replaced, for example:
print(w.workflow.inputs[0])
for input in w.workflow.inputs:
    if str(input).find("String genomeDir") > -1:
        print("found at " + input)
        newInput = 'String genomeDir = "/some/run/dir/"'
        w.source_lines[input.pos.line - 1] = newInput
        print(w.source_lines[input.pos.line - 1])
# write out to new wdl file
print(w.source_lines[0])
print("\n".join(w.source_lines))